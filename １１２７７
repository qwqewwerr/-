    $('.blog-content a').each(function(index, elem) {
        $(elem).attr('target', '_blank')
    })

    $('.blog-content img').each(function() {
        $('body').on('click', $(this), function(event) {
            var img_src = $(event.target).attr('src');
            var parent_exist = $(event.target).parents('.blog-content').length > 0
            if (img_src !== undefined && parent_exist) {
                $('.img-popup').addClass('open')
                $('.img-popup__container img').attr('src', img_src)

                $('.img-popup__container img').imagesLoaded(function() {
                    $('.img-popup__container .icon').addClass('hide')
                })
            }
        })
        var target_img = $(this).attr('src');
        $(this).css({
            'cursor': 'pointer'
        })
    })
</script>

