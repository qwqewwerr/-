            <input type="text" :name="'user-'+key" v-model.lazy="field.value" class="text-input" :id="formName+'-'+key" v-if="field.type=='text' && field.validMethod=='required' && !field.direct" @change="validation().required(key)" :placeholder="field.placeholder[lang]" @focus="focus" @blur="blur" :autocomplete="field.autocomplete">
